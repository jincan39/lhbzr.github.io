/**
 * Home.
 */
.home
  height 100%
  left 0
  position absolute
  top 0
  width 100%
  z-index 1

  &:after
    background-image url('data:image/png;base64,')
    bottom 0
    content ''
    left 0
    opacity 0.15
    position absolute
    right 0
    top 0


/**
 * Logo.
 */
.home-logo
  height 210px
  left 50%
  position absolute
  stroke #fff
  top 50%
  transform translate(-50%, -50%)
  visibility hidden
  width 210px


/**
 * Menu.
 */
.home-menu-list
  list-style none
  margin 0
  padding 0
